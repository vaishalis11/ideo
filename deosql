create table deodrand(
 deo_ id INT NOT NULL AUTO_INCREMENT,
   deo_brand VARCHAR(40) NOT NULL,
   deo_ feature VARCHAR(100) NOT NULL,
   
   PRIMARY KEY ( deodrand_id )
);

